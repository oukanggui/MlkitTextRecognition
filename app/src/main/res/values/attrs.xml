<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ViewFinderView">
        <attr name="frameHeight" format="dimension"/><!--取景框高度-->
        <attr name="frameMarginLeft" format="dimension" /><!--取景框左边缘距离屏幕左边的距离-->
        <attr name="frameMarginRight" format="dimension" /><!--取景框右边缘距离屏幕右边的距离-->
        <attr name="frameCornerRadius" format="dimension" /><!--取景框圆角半径-->
        <attr name="frameOutColor" format="color" /><!--取景框外部颜色，通常为半透明-->
    </declare-styleable>
</resources>